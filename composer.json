{
    "name": "itgalaxy/bin-version-check",
    "description": "Check whether a binary version satisfies a semver range",
    "keywords": [
        "cli",
        "bin",
        "binary",
        "executable",
        "version",
        "semver",
        "semantic",
        "range",
        "satisfy",
        "check",
        "validate"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Itgalaxy",
            "email": "info@itgalaxy.company"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "composer/semver": "^1.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.0",
        "phpunit/php-code-coverage": "^5.0.0",
        "satooshi/php-coveralls": "^1.0.0"
    },
    "scripts": {
        "phpunit": "phpunit",

        "test": "@phpunit"
    },
    "autoload": {
        "psr-4": {
            "Itgalaxy\\BinVersionCheck\\": "src/"
        }
    },
    "archive": {
        "exclude": [
            "!vendor",
            "tests",
            "*phpunit.xml*",
            ".travis.yml",
            "appveyor.yml",
            "composer.phar"
        ]
    }
}
